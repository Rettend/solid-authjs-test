{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": "./",
    "module": "ESNext",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["./src/*"]
    },
    "types": ["vite/client", "@solidjs/start/env"],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
